{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
